
@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Index category page</h1>

<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>S/N</th>
            <th>Category Name</th>
            <th>Display Order</th>
            <th>Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var cat in Model)
        {
            <tr>
                <td>@cat.Id</td>
                <td>@cat.Name</td>
                <td>@cat.DisplayOrder</td>
                <td>@cat.CreatedDateTime</td>
            </tr>
        }
    </tbody>

</table>